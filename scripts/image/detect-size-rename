
fs1=$(ls *.* | grep -ivP ".*\.jpg")


fs=$(ls *.jpg)
for f in $fs; do
    suff=$(identify -format "-%wx%h" $f)
    if [ ! -z "$suff" ]; then
        extension="${f##*.}"
        name="${f%%.$extension}"
        if [[ ! $name =~ $suff ]]; then
            cmd="mv $f $name$suff.$extension"
            echo $cmd
            $cmd
        fi
    fi
done

echo "------------"
echo fs1=$fs1
