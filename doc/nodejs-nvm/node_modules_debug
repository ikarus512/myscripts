#!/usr/bin/env node
let format = require('./node_modules/date-fns/format');

let t;

t = 'May 1 2010 01:21:01 GMT+0300 (Moscow Standard Time)';
console.log(new Date(t.getTime()));

t=1590400001000;
console.log(new Date(t));

t='2010-01-21 / 11:01:11';
console.log(new Date(t));

console.log('--');
console.log(format('2010-01-21 / 11:01:11', 'hh:mm:ss' ));          // 12:00:00
console.log(format('2010-01-21  11:01:11',  'YY-MM-DD hh:mm:ss' )); // 12:00:00
console.log(format('2010-01-21 / 11:01:11', 'YY-MM-DD hh:mm:ss' )); // 12:00:00
console.log(format('2010-01-21 11:01:11',   'YY-MM-DD hh:mm:ss' )); // ok
